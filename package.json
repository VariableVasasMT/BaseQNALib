{
  "name": "object-browser",
  "version": "1.0.1",
  "description": "A utility to access object value information using dot and array notation",
  "main": "dist/object-browser.js",
  "jsnext:main": "dist/object-browser.es.js",
  "scripts": {
    "build": "rollup -c",
    "test": "mocha ./test/test.js",
    "watch": "run-when-changed --watch \"dist/object-browser.js\" --watch \"test/test.js\" --exec \"npm test\"",
    "dev": "npm test & npm run watch & rollup -c -w"
  },
  "author": "Dmitriy A. Nesterkin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aptivator/object-browser.git"
  },
  "bugs": {
    "url": "https://github.com/aptivator/object-browser/issues"
  },
  "devDependencies": {
    "buble": "^0.15.2",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-watch": "^3.2.2",
    "run-when-changed": "^2.0.3"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
